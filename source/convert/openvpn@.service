[Unit]
Description=%p service using "%i" configuration
After=network-interfaces.service entropy.service

[Service]
PrivateTmp=true
ExecStart=%p --errors-to-stderr --suppress-timestamps --verb 3 --config ./config
LimitNPROC=10
RestartSec=30

