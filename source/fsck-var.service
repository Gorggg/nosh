[Unit]
Description=Filesystem check the /var volume
DefaultDependencies=false
EarlySupervise=true
Before=mount-var.service

[Service]
Type=oneshot
ExecStart=fsck -C -p /var
systemdWorkingDirectory=false

[Install]
WantedBy=mount-var.service
