[Unit]
Description=Kerberos v5 password socket

[Socket]
Backlog=2
BindIPv6Only=both
ListenStream=kpasswd5
Accept=true
MaxConnections=16
UCSPIRules=true
NoDelay=false

[Install]
WantedBy=server.target
