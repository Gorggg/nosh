[Unit]
Description=Set up the machine ID sysinit service
DefaultDependencies=false
EarlySupervise=true

[Service]
Type=oneshot
systemdWorkingDirectory=false
ExecStart=setup-machine-id
RemainAfterExit=yes

[Install]
WantedBy=sysinit.target
