#
# Enable basic services for the out-of-the-box PC-BSD
#
# This is not a systemd preset.
# It goes in a special nosh system-manager preset directory.
# It is used by the nosh-run-pcbsd-basic package in order to enable the services themselves.
#

#
# These services are in PCBSD but not in TrueOS.
#

# etc services
enable	automount.service
enable	automountd.service
enable	autounmountd.service
enable	devd.service
enable	devd.service
enable	devd-log.service
enable	ldconfig.service
enable	kmod@geom_uzip.service

# Normal services
enable	cyclog@kmod@acpi_video.service
enable	kmod@acpi_video.service
enable	cyclog@kmod@cuse4bsd.service
enable	kmod@cuse4bsd.service
enable	cyclog@kmod@if_bwi.service
enable	kmod@if_bwi.service
enable	cyclog@kmod@if_bwn.service
enable	kmod@if_bwn.service
enable	cyclog@kmod@iwn1000fw.service
enable	kmod@iwn1000fw.service
enable	cyclog@kmod@iwn4965fw.service
enable	kmod@iwn4965fw.service
enable	cyclog@kmod@iwn5000fw.service
enable	kmod@iwn5000fw.service
enable	cyclog@kmod@iwn5150fw.service
enable	kmod@iwn5150fw.service
enable	cyclog@kmod@iwn6000fw.service
enable	kmod@iwn6000fw.service
enable	cyclog@kmod@iwn6000g2afw.service
enable	kmod@iwn6000g2afw.service
enable	cyclog@kmod@iwn6000g2bfw.service
enable	kmod@iwn6000g2bfw.service
enable	cyclog@kmod@iwn6050fw.service
enable	kmod@iwn6050fw.service
enable	cyclog@kmod@runfw.service
enable	kmod@runfw.service
enable	cyclog@anacron.service
enable	anacron.service
enable	cyclog@avahi-daemon.service
enable	avahi-daemon.service
disable	cyclog@chkprintcap.service
disable	chkprintcap.service
enable	cyclog@org.cups.cups-cancel.service
enable	org.cups.cups-cancel.service
enable	cyclog@org.cups.cups-lpd.service
enable	org.cups.cups-lpd.service
