enable	org.cups.cupsd.service
enable	cyclog@dbus.service
enable	dbus.service
enable	cyclog@hald.service
enable	hald.service
disable	cyclog@lpd.service
disable	lpd.service
enable	cyclog@mixer@*.service
enable	mixer@*.service
enable	cyclog@moused.service
