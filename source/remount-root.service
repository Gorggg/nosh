[Unit]
Description=Remount the / filesystem (from initial read-only mount)
DefaultDependencies=false
EarlySupervise=true
Before=local-fs.target

[Service]
Type=oneshot
systemdWorkingDirectory=false
ExecStart=mount /

[Install]
WantedBy=local-fs.target
