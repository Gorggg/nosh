[Unit]
Description=SunRPC port mapping daemon

[Service]
systemdWorkingDirectory=false
ExecStart=%p -d
ExecStopPost=sh -c 'exec rm -f /run/%p.*'
Restart=always
RestartSec=1

[Install]
WantedBy=server.target
