[Unit]
Description=Service manager
After=local-fs.target

[Service]
#systemdWorkingDirectory=false
ExecStart=/bin/service-manager
TimeoutSec=10
# systemd's default file handle limit of 1024 is far too low for normal usage patterns.
LimitNOFILE=16384
