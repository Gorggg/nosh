[Unit]
Description=Combined logger for ataidle@ services

[Service]
PermissionsStartOnly=true
User=ataidle-log
WorkingDirectory=/var/log/sv
ExecStart=cyclog ataidle/
RestartSec=1

[Install]
WantedBy=workstation.target
