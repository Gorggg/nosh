[Unit]
Description=Dump kernel boot log (obsoleted by klogd-read).
DefaultDependencies=false

[Service]
Umask=0022
ExecStart=fdredir --write 1 /run/dmesg.boot %p
RemainAfterExit=yes

[Install]
# This never shuts down.
WantedBy=sysinit.target
