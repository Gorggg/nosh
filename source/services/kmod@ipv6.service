[Unit]
Description=kmod loader for ipv6

[Service]
ExecStart=sh -c '[ -d /sys/module/ipv6/ ] || system-control load-kernel-module ipv6'
RemainAfterExit=yes

[Install]
WantedBy=workstation.target
