[Unit]
Description=Set up the machine ID
DefaultDependencies=false
After=local-fs.target

[Service]
EnvironmentDirectory=env
ExecStart=setup-machine-id
RemainAfterExit=yes

[Install]
StoppedBy=shutdown.target
WantedBy=sysinit.target
