[Unit]
Description=Combined logger for natd@ services

[Service]
PermissionsStartOnly=true
User=log
WorkingDirectory=/var/log/sv
ExecStart=cyclog natd/
Restart=always
RestartSec=1

[Install]
WantedBy=workstation.target
