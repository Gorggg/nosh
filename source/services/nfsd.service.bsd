[Unit]
Description=NFS daemon (NFSv4 enabled)
Requires=kmod@nfsd
Wants=rpcbind.service mountd.service nfsuserd.service kmod@nfsd
Conflicts=nfsserver.service

[Service]
EnvironmentDirectory=env
ExecStart=%p ${flags}
Restart=never

[Install]
WantedBy=server.target
