[Unit]
Description=%P-%I server

[Service]
systemdWorkingDirectory=false
EnvironmentFile=-/etc/sysconfig/%P-%I
ExecStart=swift-%I /etc/swift/%I-server.conf
Restart=always

[Install]
WantedBy=server.target
