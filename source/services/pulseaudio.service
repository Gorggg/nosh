[Unit]
Description=PulseAudio Daemon
 
[Service]
# FIXME: Is this really needed?
systemdWorkingDirectory=on
PrivateTmp=true
ExecStart=pulseaudio --system --disallow-exit --disallow-module-loading
 
[Install]
WantedBy=multi-user.target
