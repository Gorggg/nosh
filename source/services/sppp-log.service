[Unit]
Description=Combined logger for spppcontrol@ services

[Service]
PermissionsStartOnly=true
User=sppp-log
WorkingDirectory=/var/log/sv
ExecStart=cyclog sppp/
Restart=always
RestartSec=1

[Install]
WantedBy=workstation.target
