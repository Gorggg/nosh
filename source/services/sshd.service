[Unit]
Description=OpenSSH listening daemon
Requires=sshdgenkeys.service
After=sshdgenkeys.service
Conflicts=sshd.socket

[Service]
systemdWorkingDirectory=false
RuntimeDirectory=%p
RuntimeDirectoryMode=0755
ExecStart=%p -D -e

[Install]
WantedBy=server.target
