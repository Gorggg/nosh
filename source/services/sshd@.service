[Unit]
Description=SSH per-connection service
Requires=sshdgenkeys.service
After=sshdgenkeys.service

[Service]
RuntimeDirectory=%p
RuntimeDirectoryMode=0755
ExecStart=%p -i -e
Restart=always

[Install]
WantedBy=server.target
