[Unit]
Description=Vanilla BSD system log daemon

[Service]
systemdWorkingDirectory=false
# Don't use %p; this unit is accessed via a symbolic link.
ExecStart=syslogd -c -ss
Restart=always

[Install]
WantedBy=workstation.target
