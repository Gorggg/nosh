[Unit]
Description=%p service for device %I
Wants=kmod@vkbd
After=kmod@vkbd

[Service]
EnvironmentDirectory=env
ExecStart=%p -d -v /dev/%I ${flags}
Restart=always

[Install]
WantedBy=workstation.target
