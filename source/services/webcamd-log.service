[Unit]
Description=Combined logger for webcamd@ services

[Service]
PermissionsStartOnly=true
User=webcamd-log
WorkingDirectory=/var/log/sv
ExecStart=cyclog webcamd/
Restart=always
RestartSec=1

[Install]
WantedBy=workstation.target
