[Unit]
Description=System control starting the normal target
After=service-manager.socket
After=service-manager.service

[Service]
Type=oneshot
ExecStart=/bin/system-control start --verbose normal
LimitCPU=300
TimeoutSec=600

[Install]
WantedBy=multi-user.target
