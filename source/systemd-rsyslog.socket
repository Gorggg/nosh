[Unit]
Description=systemd journal output to ryslog socket
#Wants=syslog.target

[Socket]
ListenDatagram=/run/systemd/journal/syslog
SocketMode=0666
PassCredentials=yes
PassSecurity=yes

[Install]
WantedBy=workstation.target
